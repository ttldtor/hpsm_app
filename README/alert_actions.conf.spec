[hpsm]

param.url = <string>
* HPSM REST API URL

param.login = <string>
* HPSM user login

param.password = <string>
* HPSM user password

param.category = <string>
* HPSM incident category (default: incident)

param.subcategory = <string>
* HPSM incident subcategory

param.productType = <string>
* HPSM incident productType

param.from = <string>
* HPSM incident service name (default: splunk)

param.affectedItem = <string>
* HPSM incident affected item (default: CI1127974)

param.location = <string>
* HPSM incident location (default: 5a12997da100a05f506e1e38)

param.callbackContact = <string>
* HPSM incident callback contact (default: splunk)

param.contactName = <string>
* HPSM incident contact name (default: splunk)

param.assignment = <string>
* HPSM incident assignment (default: SPLUNK)
